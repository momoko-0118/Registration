<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->

<struts>
	<constant name="struts.devMode" value="true" />
	<package name="com.diworksdev.Registration.action" extends="struts-default">
	
		<action name="GoHomeAction" 
		class="com.diworksdev.Registration.action.GoHomeAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>
		
		<action name="RegistAction" 
		class="com.diworksdev.Registration.action.RegistAction" 
		method="execute">
			<result name="success">regist.jsp</result>
		</action>
		
		<action name="RegistConfirmAction" 
		class="com.diworksdev.Registration.action.RegistConfirmAction"
		method="execute">
			<result name="success">registConfirm.jsp</result>
			<result name="error">regist.jsp</result>
		</action>
		
		<action name="RegistCompleteAction" 
		class="com.diworksdev.Registration.action.RegistCompleteAction"
		method="execute">
			<result name="success">registComplete.jsp</result>
			<result name="error">registConfirm.jsp</result>
		</action>
	</package>
</struts>